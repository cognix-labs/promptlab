# OpenRouter API key for accessing various AI models
OPENROUTER_API_KEY=your_openrouter_api_key_here

# OpenRouter model to use for inference (default: openai/gpt-3.5-turbo)
# Examples: openai/gpt-4, anthropic/claude-3-opus, deepseek/deepseek-chat-v3-0324
OPENROUTER_MODEL=openai/o4-mini

# OpenRouter model to use for embeddings (default: same as OPENROUTER_MODEL)
# OPENROUTER_EMBEDDING_MODEL=openai/gpt-3.5-turbo

# Port for PromptLab Studio (default: 8000)
# PROMPTLAB_PORT=8000
